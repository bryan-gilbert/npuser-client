{
  "name": "npuser-client",
  "version": "1.0.5",
  "description": "Module for a server that uses the npuser service (the no password authentication service).",
  "repository": {
    "type": "git",
    "url": "npuser-client"
  },
  "keywords": [
    "api",
    "auth",
    "authentication",
    "npuser",
    "password",
    "passwordless"
  ],
  "author": "Bryan Gilbert",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "eslint ./src/**",
    "run": "ts-node src/index.ts",
    "test": "mocha test/**/*.spec.js",
    "test-ts": "ts-mocha -p tsconfig.json src/**/*.spec.ts",
    "test:cov": "nyc ts-mocha -p tsconfig.json src/**/*.spec.ts"
  },
  "dependencies": {
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "14.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
